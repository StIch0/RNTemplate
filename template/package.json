    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lodash.debounce": "^4.0.8",
    "metro-react-native-babel-preset": "^0.70.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "18.0.0",
