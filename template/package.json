{
  "name": "RNTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "reverse": "adb reverse tcp:8081 tcp:8081",
    "start:clean": "watchman watch-del-all && yarn start cache clean",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write \"./**/*.{ts,tsx}\"",
    "husky": "yarn pretty-quick --staged && npx lint-staged && yarn tsc",
    "create-toolkit-dir": "./scripts/createToolkitDir.sh",
    "prepare": "husky",
    "postinstall": "npx pod-install",
    "pod-install": "pod install",
    "android:assembleRelease": "cd ./android && ./gradlew clean && ./gradlew assembleRelease && open ./app/build/outputs/apk",
    "android:bundleRelease": "cd ./android && ./gradlew clean && ./gradlew bundleRelease && open ./app/build/outputs/bundle"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.22.3",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "@reduxjs/toolkit": "^2.2.1",
    "eslint-config-prettier": "^9.1.0",
    "immer": "^10.0.3",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.2.0",
    "react-hook-form": "^7.50.1",
    "react-native": "0.73.5",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-text-input-mask": "^3.2.0",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "reselect": "^5.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.74.0",
    "@tsconfig/react-native": "^3.0.3",
    "@types/jest": "^29.5.12",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.2.60",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.0.7",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-ft-flow": "^3.0.5",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-order": "^2.1.4",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lodash.debounce": "^4.0.8",
    "prettier": "3.2.5",
    "pretty-quick": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
